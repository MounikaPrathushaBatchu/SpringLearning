<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"https://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="colldata" class="beans.CollectionData">
		<property name="fruits">
			<list>
				<value>Mango</value>
				<value>Apple</value>
				<value>Grape</value>
				<value>Banana</value>
				<value>Cherry</value>
				<value>Apple</value>
			</list>
		</property>
		<property name="mobileNumbers">
			<set>
				<value>1234567890</value>
				<value>1234567890</value>
				<value>9876543210</value>
				<value>1236547890</value>
			</set>
		</property>
		<property name="edata">
			<map>
				<entry key="01" value="A" />
				<entry key="02" value="B" />
				<entry key="03" value="C" />
				<entry key="04" value="D" />
				<entry key="05" value="E" />
			</map>
		</property>
	</bean>
</beans>