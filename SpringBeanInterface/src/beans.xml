<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.syena.beanInterface.HelloWorld" id="helloWorld">
		<property name="msg1" value="World Hello" />
		<property name="msg2" value="abc" />
	</bean>

	<bean class="com.syena.beanInterface.HelloIndia" id="helloIndia" parent="helloWorld">
		<property name="msg1" value="Hello India" />
		<property name="msg3" value="def" />
		<property name="msg2" value="xyz" />
	</bean>

</beans>